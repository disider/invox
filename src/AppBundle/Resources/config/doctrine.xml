<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="document_template_per_company_repository" class="AppBundle\Entity\Repository\DocumentTemplatePerCompanyRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\DocumentTemplatePerCompany</argument>
        </service>

        <service id="invite_repository" class="AppBundle\Entity\Repository\EntityRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\Invite</argument>
        </service>

        <service id="medium_repository" class="AppBundle\Entity\Repository\EntityRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\Medium</argument>
        </service>

        <service id="page_repository" class="AppBundle\Entity\Repository\EntityRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\Page</argument>
        </service>

        <service id="paragraph_template_repository" class="AppBundle\Entity\Repository\ParagraphTemplateRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\ParagraphTemplate</argument>
        </service>

        <service id="payment_type_repository" class="AppBundle\Entity\Repository\EntityRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\PaymentType</argument>
        </service>

        <service id="petty_cash_note_repository" class="AppBundle\Entity\Repository\PettyCashNoteRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\PettyCashNote</argument>
        </service>

        <service id="product_repository" class="AppBundle\Entity\Repository\ProductRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\Product</argument>
        </service>

        <service id="province_repository" class="AppBundle\Entity\Repository\EntityRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\Province</argument>
        </service>

        <service id="recurrence_repository" class="AppBundle\Entity\Repository\RecurrenceRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\Recurrence</argument>
        </service>

        <service id="service_repository" class="AppBundle\Entity\Repository\ServiceRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\Service</argument>
        </service>

        <service id="tax_rate_repository" class="AppBundle\Entity\Repository\EntityRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\TaxRate</argument>
        </service>

        <service id="user_repository" class="AppBundle\Entity\Repository\EntityRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\User</argument>
        </service>

        <service id="warehouse_record_repository" class="AppBundle\Entity\Repository\WarehouseRecordRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\WarehouseRecord</argument>
        </service>

        <service id="working_note_repository" class="AppBundle\Entity\Repository\WorkingNoteRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\WorkingNote</argument>
        </service>

        <service id="zip_code_repository" class="AppBundle\Entity\Repository\ZipCodeRepository">
            <factory service="doctrine.orm.default_entity_manager" method="getRepository"/>
            <argument>AppBundle\Entity\ZipCode</argument>
        </service>
    </services>

</container>
