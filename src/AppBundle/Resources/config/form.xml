<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="country_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\CountryForm</argument>
            <argument type="service" id="country_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="customer_form_processor" class="AppBundle\Form\Processor\CustomerFormProcessor" public="true">
            <argument type="service" id="customer_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="document_form_processor" class="AppBundle\Form\Processor\DocumentFormProcessor" public="true">
            <argument type="service" id="document_repository"/>
            <argument type="service" id="company_manager"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
            <argument>%available_locales%</argument>
        </service>

        <service id="document_template_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\DocumentTemplateForm</argument>
            <argument type="service" id="document_template_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="document_template_per_company_form_processor" class="AppBundle\Form\Processor\DefaultAuthenticatedFormProcessor" public="true">
            <argument>AppBundle\Form\DocumentTemplatePerCompanyForm</argument>
            <argument type="service" id="document_template_per_company_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="invite_form_processor" class="AppBundle\Form\Processor\InviteFormProcessor" public="true">
            <argument type="service" id="invite_repository"/>
            <argument type="service" id="default_mailer"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="medium_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\MediumForm</argument>
            <argument type="service" id="medium_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="page_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\PageForm</argument>
            <argument type="service" id="page_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="paragraph_template_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\ParagraphTemplateForm</argument>
            <argument type="service" id="paragraph_template_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="payment_type_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\PaymentTypeForm</argument>
            <argument type="service" id="payment_type_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="petty_cash_note_form_processor" class="AppBundle\Form\Processor\PettyCashNoteFormProcessor" public="true">
            <argument type="service" id="petty_cash_note_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="product_form_processor" class="AppBundle\Form\Processor\DefaultAuthenticatedFormProcessor" public="true">
            <argument>AppBundle\Form\ProductForm</argument>
            <argument type="service" id="product_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="province_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\ProvinceForm</argument>
            <argument type="service" id="province_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="recurrence_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\RecurrenceForm</argument>
            <argument type="service" id="recurrence_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="service_form_processor" class="AppBundle\Form\Processor\DefaultAuthenticatedFormProcessor" public="true">
            <argument>AppBundle\Form\ServiceForm</argument>
            <argument type="service" id="service_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="tax_rate_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\TaxRateForm</argument>
            <argument type="service" id="tax_rate_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="user_form_processor" class="AppBundle\Form\Processor\UserFormProcessor" public="true">
            <argument type="service" id="user_manager"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="warehouse_record_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\WarehouseRecordForm</argument>
            <argument type="service" id="warehouse_record_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="working_note_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\WorkingNoteForm</argument>
            <argument type="service" id="working_note_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>

        <service id="zip_code_form_processor" class="AppBundle\Form\Processor\DefaultFormProcessor" public="true">
            <argument>AppBundle\Form\ZipCodeForm</argument>
            <argument type="service" id="zip_code_repository"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="security.token_storage"/>
        </service>


        <service id="attachment_file_type" class="AppBundle\Form\Type\AttachmentFileType" public="true">
            <tag name="form.type"/>
        </service>

        <service id="country_entity_type" class="AppBundle\Form\Type\CountryEntityType" public="true">
            <argument>%locale%</argument>
            <tag name="form.type"/>
        </service>

        <service id="document_row_collection_type" class="AppBundle\Form\Type\DocumentRowCollectionType" public="true">
            <argument type="service" id="locale_formatter"/>
            <tag name="form.type"/>
        </service>

        <service id="document_row_type" class="AppBundle\Form\Type\DocumentRowType" public="true">
            <argument type="service" id="locale_formatter"/>
            <tag name="form.type"/>
        </service>

        <service id="entity_hidden_type" class="AppBundle\Form\Type\EntityHiddenType" public="true">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <tag name="form.type"/>
        </service>

        <service id="entity_text_type" class="AppBundle\Form\Type\EntityTextType" public="true">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <tag name="form.type"/>
        </service>

        <service id="image_file_type" class="AppBundle\Form\Type\ImageFileType" public="true">
            <tag name="form.type"/>
        </service>

        <service id="invoice_collection_type" class="AppBundle\Form\Type\InvoiceCollectionType" public="true">
            <argument type="service" id="company_manager"/>
            <tag name="form.type"/>
        </service>

        <service id="invoice_per_note_type" class="AppBundle\Form\Type\InvoicePerNoteType" public="true">
            <tag name="form.type"/>
        </service>

        <service id="localized_number_type" class="AppBundle\Form\Type\LocalizedNumberType" public="true">
            <argument type="service" id="locale_formatter"/>
            <tag name="form.type"/>
        </service>
    </services>

</container>
