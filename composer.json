{
  "name": "invox/main",
  "license": "MIT",
  "type": "project",
  "description": "Invox Project",
  "autoload": {
    "psr-4": {
      "AdminBundle\\": "src/AdminBundle/",
      "AppBundle\\": "src/AppBundle/",
      "App\\": "src/"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\AppBundle\\": "tests/AppBundle/",
      "Tests\\App\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "jquery/jquery",
        "version": "1.11.1",
        "dist": {
          "url": "https://code.jquery.com/jquery-1.11.1.js",
          "type": "file"
        }
      }
    },
    {
      "type": "git",
      "url": "https://github.com/disider/GeneratorBundle.git"
    },
    {
      "type": "git",
      "url": "https://github.com/disider/BehatExtension.git"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "a2lix/translation-form-bundle": "^3.0",
    "doctrine/annotations": "^1.6",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "doctrine/doctrine-migrations-bundle": "^2.0",
    "doctrine/orm": "^2.5",
    "excelwebzone/recaptcha-bundle": "^1.5",
    "guzzlehttp/guzzle": "^6.3",
    "jms/i18n-routing-bundle": "^3.0",
    "jms/serializer-bundle": "^3.1",
    "jms/translation-bundle": "^1.4",
    "knplabs/doctrine-behaviors": "^1.6",
    "knplabs/knp-menu-bundle": "^2.2",
    "knplabs/knp-paginator-bundle": "^3.0",
    "knplabs/knp-snappy-bundle": "^1.6",
    "lexik/form-filter-bundle": "^5.0",
    "liip/imagine-bundle": "^2.1",
    "nelmio/api-doc-bundle": "^3.4",
    "oneup/uploader-bundle": "^2.1",
    "sensio/framework-extra-bundle": "^5.0.0",
    "sonata-project/admin-bundle": "^3.43",
    "sonata-project/core-bundle": "^3.13",
    "sonata-project/doctrine-orm-admin-bundle": "^3.7",
    "symfony/asset": "4.2.*",
    "symfony/console": "^4.2",
    "symfony/debug-pack": "^1.0",
    "symfony/dotenv": "^4.2",
    "symfony/flex": "^1.2",
    "symfony/form": "4.2.*",
    "symfony/framework-bundle": "^4.2",
    "symfony/monolog-bundle": "^3.1.0",
    "symfony/polyfill-apcu": "^1.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/security-csrf": "4.2.*",
    "symfony/serializer": "^4.0",
    "symfony/serializer-pack": "^1.0",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/translation": "4.2.*",
    "symfony/twig-bundle": "4.2.*",
    "symfony/validator": "4.2.*",
    "symfony/yaml": "^4.2",
    "twbs/bootstrap": "^4.3",
    "twig/extensions": "^1.5",
    "twig/twig": "^1.0||^2.0"
  },
  "require-dev": {
    "behat/behat": "^3.5",
    "behat/mink": "dev-master",
    "behat/mink-browserkit-driver": "^1.3",
    "behat/mink-extension": "^2.3",
    "behat/mink-selenium2-driver": "^1.3",
    "behat/symfony2-extension": "^2.1",
    "disider/behat-extension": "^2.1",
    "dmore/behat-chrome-extension": "^1.2",
    "doctrine/data-fixtures": "^1.3",
    "hautelook/alice-bundle": "^2.3",
    "polishsymfonycommunity/symfony2-mocker-extension": "^2.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/web-server-bundle": "^4.0",
    "theofidry/alice-data-fixtures": "^1.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.2.*"
    }
  }
}
